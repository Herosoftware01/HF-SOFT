<configuration>
  <system.webServer>
    <handlers>
      <add name="PythonHandler" 
           path="*" 
           verb="*" 
           modules="FastCgiModule"
           scriptProcessor="C:\inetpub\wwwroot\roll-herofashion-com\herofashion\env\Scripts\python.exe|C:\inetpub\wwwroot\roll-herofashion-com\herofashion\env\Lib\site-packages\wfastcgi.py"

           resourceType="Unspecified" 
           requireAccess="Script" />
    </handlers>

    <rewrite>
      <rules>
        <rule name="DjangoApp" stopProcessing="true">
          <match url="(.*)" />
          <action type="Rewrite" url="wfastcgi.py" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>

  <appSettings>
    <!-- Update this to match your Django project package (the folder with wsgi.py) -->
    <add key="WSGI_HANDLER" value="roll_check.wsgi.application" />

    <!-- Path to your Django project root (where manage.py lives) -->
    <add key="PYTHONPATH" value="C:\inetpub\wwwroot\roll-herofashion-com\herofashion" />
  </appSettings>
</configuration>